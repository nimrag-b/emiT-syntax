{
  "name": "emit-lang",
  "displayName": "emiT Lang",
  "description": "Syntax Highlighting for emiT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "emit",
      "aliases": ["emiT", "emit"],
      "extensions": [".emit"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "emit",
      "scopeName": "source.emit",
      "path": "./syntaxes/emit.tmLanguage.json"
    }]
  }
}
